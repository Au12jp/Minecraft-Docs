      },
      "minecraft:healable": {
        "items": [
          {
            "heal_amount": 2,
            "item": "cactus"
          }
        ]
      },
      "minecraft:health": {
        "value": 32
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "cause": "lava",
            "damage_per_tick": 4,
            "filters": {
              "operator": "==",
              "test": "in_lava",
              "subject": "self",
              "value": true
            }
          }
        ]
      },
      "minecraft:is_hidden_when_invisible": {},
